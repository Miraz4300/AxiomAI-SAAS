# Release and version
VITE_GLOB_APP_NAME=$npm_package_name
VITE_GLOB_APP_ENVIRONMENT=$NODE_ENV
VITE_GLOB_APP_VERSION=$npm_package_version

# Glob APP & API URL
VITE_GLOB_API_URL=/axiomnode
VITE_APP_API_BASE_URL=http://127.0.0.1:10829/

# Whether PWA is enabled during build
VITE_GLOB_APP_PWA=false

# Whether long replies are supported, which may result in higher API fees
VITE_GLOB_OPEN_LONG_REPLY=true

# Enable speech if use https
VITE_ENABLE_SPEECH=true