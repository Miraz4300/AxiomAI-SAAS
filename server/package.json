{
  "name": "axiomnode",
  "version": "2.4.0-stable",
  "private": false,
  "description": "AxiomNode",
  "author": "Miraz4300 <miraz4300@gmail.com>",
  "keywords": [
    "axiomai",
    "axiomnode",
    "express"
  ],
  "engines": {
    "node": "^16 || ^18 || ^19"
  },
  "scripts": {
    "start": "esno ./src/index.ts",
    "dev": "esno watch ./src/index.ts",
    "prod": "esno ./build/index.js",
    "build": "pnpm clean && tsup",
    "clean": "rimraf build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "common:cleanup": "rimraf node_modules && rimraf pnpm-lock.yaml"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "chatgpt": "^5.2.4",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "esno": "^0.16.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "https-proxy-agent": "^5.0.1",
    "isomorphic-fetch": "^3.0.0",
    "jwt-decode": "^3.1.2",
    "mongodb": "^5.5.0",
    "node-fetch": "^3.3.1",
    "nodemailer": "^6.9.2",
    "socks-proxy-agent": "^7.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.1",
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.3",
    "eslint": "^8.41.0",
    "jsonwebtoken": "^9.0.0",
    "rimraf": "^5.0.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  }
}
