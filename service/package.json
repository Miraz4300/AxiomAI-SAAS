{
  "name": "axiomnode",
  "type": "module",
  "version": "3.2.1",
  "private": false,
  "description": "AxiomNode",
  "author": "Miraz4300 <miraz4300@gmail.com>",
  "keywords": [
    "axiomai",
    "axiomnode",
    "express"
  ],
  "engines": {
    "node": "^20 || ^22"
  },
  "scripts": {
    "start": "tsx ./src/index.ts",
    "dev": "tsx watch ./src/index.ts",
    "prod": "node --import tsx/esm ./build/index.js",
    "prod-node18": "node --loader tsx/esm ./build/index.js",
    "build": "pnpm clean && tsc && pnpm copy",
    "copy": "copyfiles -u 1 src/utils/templates/* build",
    "clean": "rimraf build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "common:cleanup": "rimraf node_modules && rimraf pnpm-lock.yaml"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "bull": "^4.16.5",
    "chatgpt": "npm:@chatgptweb/chatgpt-api@5.3.0",
    "connect-history-api-fallback": "^2.0.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "file-type": "^19.6.0",
    "gpt-token": "^0.0.5",
    "ioredis": "^5.6.0",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^3.1.2",
    "mongodb": "^6.15.0",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.0",
    "request-ip": "^3.3.0",
    "rotating-file-stream": "^3.2.6",
    "speakeasy": "^2.0.0",
    "tsx": "^4.19.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.43.1",
    "@types/express": "^4.17.21",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.17.30",
    "@types/nodemailer": "^6.4.17",
    "@types/request-ip": "^0.0.41",
    "@types/speakeasy": "^2.0.10",
    "copyfiles": "^2.4.1",
    "eslint": "8.56.0",
    "rimraf": "^5.0.10",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  }
}
