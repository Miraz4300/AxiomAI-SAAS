{
  "name": "axiomnode",
  "type": "module",
  "version": "3.0.0-RC5",
  "private": false,
  "description": "AxiomNode",
  "author": "Miraz4300 <miraz4300@gmail.com>",
  "keywords": [
    "axiomai",
    "axiomnode",
    "express"
  ],
  "engines": {
    "node": "^18 || ^19 || ^20"
  },
  "scripts": {
    "start": "tsx ./src/index.ts",
    "dev": "tsx watch ./src/index.ts",
    "prod": "node --import tsx/esm ./build/index.js",
    "prod-node18": "node --loader tsx/esm ./build/index.js",
    "build": "pnpm clean && tsc",
    "clean": "rimraf build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "common:cleanup": "rimraf node_modules && rimraf pnpm-lock.yaml"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "chatgpt": "^5.2.5",
    "connect-history-api-fallback": "^2.0.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "gpt-token": "^0.0.5",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^3.1.2",
    "mongodb": "^6.3.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "request-ip": "^3.3.0",
    "rotating-file-stream": "^3.2.1",
    "speakeasy": "^2.0.0",
    "tsx": "^4.7.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.43.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "eslint": "^8.56.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
